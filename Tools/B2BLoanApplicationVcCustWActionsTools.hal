    end else begin
      if (profit<ECb.IncomeStep2) then begin
        res = ECb.Enterpreneur900;
      end else begin
        res = ECb.Enterpreneur9002;
      end;
    end;
  end;

  LogValue("Q91-maj","Pajamos",res,ECb.Debug);
  
  CalculatePersonCoefMaj = res;
  return;
end;


