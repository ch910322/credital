begin
  val bx,bx1,res,surval,bx2; 
  Integer specialist,surnum;
  string 255 tmp; 
  val T48

  specialist = 0;


  tmp = GetFieldValueByName(LAr,"Surety" & surety & "Type",-1);
  if (tmp=="1") then begin
